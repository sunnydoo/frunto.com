<!--page/frunto/swinetools.wxml-->
<view class="container">
      <swiper indicator-dots="true"  style='height:1200rpx' >
          <swiper-item>
            <scroll-view>
              <view class="table">
              
  <view class="tr bg-w">
          <view class="td-name">{{item.name}}</view>
          <view class="td" wx:for="{{headerItems}}">{{item}}</view>
  </view>

  <block wx:for="{{rowsOfKnows}}">
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
      <view class="td-name">{{item.name}}</view>
      <view class="td">{{item.mild}}</view>
      <view class="td">{{item.moderate}}</view>
      <view class="td">{{item.severe}}</view>
    </view>
    <view class="tr" wx:else>
      <view class="td-name">{{item.name}}</view>
      <view class="td">{{item.mild}}</view>
      <view class="td">{{item.moderate}}</view>
      <view class="td">{{item.severe}}</view>
    </view>
  </block>
</view>
            </scroll-view>
          </swiper-item>
       <swiper-item>
            <scroll-view>
              <view class="table">
  <view class="tr bg-w">
    <view class="th">head1</view>
    <view class="th">head2</view>
    <view class="th ">head3</view>
  </view>
  <block wx:for="{{rowsoofKnows}}" >
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
      <view class="td">{{item.code}}</view>
      <view class="td">{{item.text}}</view>
      <view class="td">{{item.type}}</view>
    </view>
    <view class="tr" wx:else>
      <view class="td">{{item.code}}</view>
      <view class="td">{{item.text}}</view>
      <view class="td">{{item.type}}</view>
    </view>
  </block>
</view>
            </scroll-view>
          </swiper-item>
             <swiper-item>
            <scroll-view>
              <view class="table">
  <view class="tr bg-w">
    <view class="th">head1</view>
    <view class="th">head2</view>
    <view class="th ">head3</view>
  </view>
  <block wx:for="{{listData}}" wx:key="{{code}}">
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
      <view class="td">{{item.code}}</view>
      <view class="td">{{item.text}}</view>
      <view class="td">{{item.type}}</view>
    </view>
    <view class="tr" wx:else>
      <view class="td">{{item.code}}</view>
      <view class="td">{{item.text}}</view>
      <view class="td">{{item.type}}</view>
    </view>
  </block>
</view>
            </scroll-view>
          </swiper-item>
      </swiper>
</view>
